<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="10"
    tools:context=".User.Activities.UDashboardActivityNew">

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="9.1"></FrameLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.01"
        android:background="@drawable/icon_rectangle_bar_gray" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.9"
        android:background="@color/white"
        android:orientation="horizontal"
        android:weightSum="10">

        <LinearLayout
            android:id="@+id/ll_one"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical"
            android:weightSum="10">

            <View
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.1" />

            <ImageView
                android:id="@+id/iv_account"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:paddingTop="7dp"
                android:src="@drawable/resto_black" />

            <com.beachbox.beachbox.widgets.TextViewLight
                android:id="@+id/tv_tabText1"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:gravity="center"
                android:text="Restaurants"
                android:textSize="10sp" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2">

            <Button
                android:id="@+id/btnCartCount"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_marginLeft="41dp"
                android:layout_marginTop="1dp"
                android:background="@drawable/shape_notification"
                android:gravity="center"
                android:text="0"

                android:textColor="@color/white"
                android:textSize="8sp" />

            <LinearLayout
                android:id="@+id/ll_two"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical"
                android:weightSum="10">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.1" />

                <ImageView
                    android:id="@+id/iv_cart"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="5"
                    android:paddingTop="7dp"
                    android:src="@drawable/cart_black" />

                <com.beachbox.beachbox.widgets.TextViewLight
                    android:id="@+id/tv_tabText2"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:text="Cart"
                    android:textSize="10sp" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/ll_three"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical"
            android:weightSum="10">

            <View
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.1" />

            <ImageView
                android:id="@+id/iv_account2"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:paddingTop="7dp"
                android:src="@drawable/order_black" />

            <com.beachbox.beachbox.widgets.TextViewLight
                android:id="@+id/tv_tabText3"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:gravity="center"
                android:text="Orders"
                android:textSize="10sp" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2">

            <!--  <Button
                  android:id="@+id/btnNotiCount"
                  android:layout_width="18dp"
                  android:layout_height="18dp"
                  android:textSize="8sp"
                  android:gravity="center"
                  android:textColor="@color/white"
                  android:background="@drawable/shape_notification"
                  android:layout_marginLeft="41dp"
                  android:text="0"
                  android:layout_marginTop="2dp" />-->
            <LinearLayout
                android:id="@+id/ll_noti"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical"
                android:weightSum="10">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.1" />

                <ImageView
                    android:id="@+id/tabImageNoti"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="5"
                    android:paddingTop="7dp"
                    android:src="@drawable/noti_black" />

                <com.beachbox.beachbox.widgets.TextViewLight
                    android:id="@+id/tv_tabNotiText"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="4"
                    android:gravity="center"
                    android:text="Notifications"
                    android:textSize="10sp" />
            </LinearLayout>
        </RelativeLayout>


        <LinearLayout
            android:id="@+id/ll_four"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2.5"
            android:orientation="vertical"
            android:weightSum="10">

            <View
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.1" />

            <ImageView
                android:id="@+id/iv_account3"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:paddingTop="7dp"
                android:src="@drawable/account_black" />

            <com.beachbox.beachbox.widgets.TextViewLight
                android:id="@+id/tv_tabText4"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="4"
                android:gravity="center"
                android:text="Account"
                android:textSize="10sp" />
        </LinearLayout>

    </LinearLayout>
</LinearLayout>
